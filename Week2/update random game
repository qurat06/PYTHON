import random
count=5
n=random.randint(1,100)
while (count>0):
    x=int(input("please enter the number"))
    if x<n:
        print("too low")
    elif x>n:
        print("too high")
    else:
        print("congrats u got the right number")
        break;
        count-=1
        print("you lost")
